def profile(name, age, *language): # *language = when the number of language is not determined
    print("Your name is {}\tYour age is :{}\t".format(name,age),end=" ") #end=" " < ends with " " 
    for lang in language:
        print(lang,end=" ")
    print()

profile("James",20,"Python","Java","C","C++","C#","JavaScript")
profile("Kim",25,"kotlin","swift",)
